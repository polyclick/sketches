{
  "name": "polyclick-starter",
  "version": "0.2.0",
  "description": "Very opinionated starter (es6+, babel, gulp, jspm, eslint, sass)",
  "main": "js/app.js",
  "homepage": "https://github.com/polyclick/polyclick-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyclick/polyclick-starter.git"
  },
  "bugs": "https://github.com/polyclick/polyclick-starter/issues",
  "author": {
    "name": "Bart Claessens",
    "email": "hello@polyclick.io",
    "url": "https://polyclick.io"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss",
    "postinstall": "jspm install"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "boilerplate",
    "babel",
    "systemjs",
    "threejs",
    "stackgl",
    "pixi.js"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "del": "^2.2.2",
    "esdoc": "^0.5.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.0.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.2.0",
    "gulp-jspm": "^0.5.13",
    "gulp-minify-html": "^1.0.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "imagemin-pngquant": "^5.0.0",
    "jspm": "^0.17.0-beta.41",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jspm": "^2.2.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sinon-chai": "^1.3.1",
    "mocha": "^3.2.0",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "sassdoc": "^2.2.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "dat.gui": "npm:dat.gui@^0.6.1",
      "gsap": "npm:gsap@^1.18.0",
      "three": "github:mrdoob/three.js@master"
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.21"
    },
    "peerDependencies": {
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0"
    },
    "overrides": {
      "github:mrdoob/three.js@master": {
        "format": "global"
      },
      "npm:babel-runtime@5.8.35": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      }
    }
  },
  "dependencies": {}
}
