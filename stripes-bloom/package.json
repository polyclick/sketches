{
  "name": "polyclick-threejs-starter",
  "version": "0.1.0",
  "description": "Three.js boilerplate for ES6+ apps using JSPM + Babel",
  "main": "dist/index.html",
  "homepage": "https://github.com/polyclick/polyclick-threejs-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyclick/polyclick-threejs-starter.git"
  },
  "bugs": "https://github.com/polyclick/polyclick-threejs-starter/issues",
  "author": {
    "name": "Bart Claessens",
    "email": "hello@polyclick.io",
    "url": "http://polyclick.io"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "buildjs": "jspm bundle-sfx js/app dist/app.min.js --minify --skip-source-maps",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "boilerplate",
    "babel",
    "systemjs",
    "threejs"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "del": "^2.0.2",
    "esdoc": "^0.4.3",
    "eslint": "^1.9.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^1.1.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.1.0",
    "gulp-scss-lint": "^0.3.8",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^4.2.0",
    "jspm": "^0.16.24",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.3",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
    "sassdoc": "^2.1.15"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "gsap": "npm:gsap@^1.18.0",
      "jquery": "npm:jquery@^2.2.0",
      "lodash": "npm:lodash@^4.0.0",
      "three": "github:mrdoob/three.js@master"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
